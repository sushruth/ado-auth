{
	"name": "ado-auth",
	"version": "0.2.4",
	"displayName": "ADO Authenticator",
	"license": "MIT",
	"main": "./bin/src/cli.js",
	"types": "./bin/types.d.ts",
	"keywords": ["azure devops", "authenticate", "vsts auth", "ado-auth", "ado auth"],
	"scripts": {
		"compile": "node ./scripts/build.mjs",
		"typegen": "tsc",
		"build": "yarn compile && yarn typegen",
		"dev": "esr ./src/cli.ts",
		"prepublish": "yarn build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sushruth/ado-auth-cli"
	},
	"bin": "./bin/src/cli.js",
	"dependencies": {
		"chalk": "4.1.2",
		"dayjs": "1.10.7",
		"js-yaml": "4.1.0",
		"opener": "1.5.2",
		"ora": "6.0.1",
		"sade": "1.7.4"
	},
	"devDependencies": {
		"@types/js-yaml": "4.0.0",
		"@types/node": "16.11.10",
		"@types/opener": "1.4.0",
		"@types/sade": "1.7.3",
		"@yarnpkg/esbuild-plugin-pnp": "2.0.0",
		"esbuild": "0.13.15",
		"esbuild-runner": "2.2.1",
		"typescript": "4.5.2"
	},
	"engines": {
		"node": ">=12.x.x"
	},
	"packageManager": "yarn@3.2.0-rc.5"
}
